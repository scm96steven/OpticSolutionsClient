@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">

<head>
    <title>
        @*@if (!string.IsNullOrWhiteSpace(ViewBag.Title))
        {
            @ViewBag.Title@: -
        }*@
        OpticSolutions
    </title>
    <link rel="icon" href="@Url.Content("~/Content/favicon.ico")" />
    <!-- Bootstrap -->
   <link href="~/Content/css/AdminLTE.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/css/animate.css">
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/css/font-awesome.css">
    <link rel="stylesheet" href="~/Content/css/jquery.bxslider.css">
    <link rel="stylesheet" type="~/Content/text/css" href="css/normalize.css" />
    <link rel="stylesheet" type="~/Content/text/css" href="css/demo.css" />
    <link rel="stylesheet" type="~/Content/text/css" href="css/set1.css" />
    <link href="~/Content/css/overwrite.css" rel="stylesheet">
    <link href="~/Content/css/style.css" rel="stylesheet">

    <!-- =======================================================
      Theme Name: eNno
      Theme URL: https://bootstrapmade.com/enno-free-simple-bootstrap-template/
      Author: BootstrapMade
      Author URL: https://bootstrapmade.com
    ======================================================= -->
</head>

<body>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse.collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index","Home")" ><span><img src="~/img/Optic solutions logo.png" style="height:50px; width:auto"/><strong>Optic</strong>Solutions</span></a>
            </div>
            <div class="navbar-collapse collapse">
                <div class="menu">
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation"><a href="@Url.Action("Index","Home")">Inicio</a></li>
                        <li role="presentation"><a href="@Url.Action("Services","Home")">Servicios</a></li>
                        <li role="presentation"><a href="@Url.Action("CrearCita","Appointment")">Solicitud de Cita</a></li>
                        <li role="presentation"><a href="@Url.Action("Status","Home")">Estado de orden</a></li>
                        <li role="presentation" @*class="active"*@><a href="@Url.Action("Contact","Home")">Contacto</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>

    <div class="row">
        @RenderBody()
    </div>

    <footer>
        <div class="inner-footer">
            <div class="container">
                <div class="row">

                    <div class="col-md-4 f-contact">
                        <h3 class="widgetheading">Nuesto contacto</h3>
                        <a href="#">
                            <p><i class="fa fa-envelope"></i>OpticSolutions@gmail.com</p>
                        </a>
                        <p><i class="fa fa-phone"></i> 809-841-8579</p>

                        <p><i class="fa fa-home"></i> Optic Solutions | Calle Francisco Prats Ramirez #859, 
                        El Millon 10149, Santo Domingo, República Dominicana <br /></p>
                    </div>
                </div>
            </div>
        </div>

        <div class="last-div">
      
            <a href="" class="scrollup"><i class="fa fa-chevron-up"></i></a>
        </div>
    </footer>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/Content/js/jquery-2.1.1.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/jquery.easing.1.3.js"></script>
    <script src="~/Content/js/jquery.isotope.min.js"></script>
    <script src="~/Content/js/jquery.bxslider.min.js"></script>
    <script type="~/Content/text/javascript" src="js/fliplightbox.min.js"></script>
    <script src="~/Content/js/functions.js"></script>

    @*<script type="text/javascript">
    $('.portfolio').flipLightBox()
    </script>*@


</body>

</html>
